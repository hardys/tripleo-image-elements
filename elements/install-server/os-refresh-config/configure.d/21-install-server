
if [ ! -e ~/.ssh/id_rsa ] ; then
    ssh-keygen -t rsa -N ""  -f ~/.ssh/id_rsa
    cat ~/.ssh/id_rsa.pub >> ~/.ssh/authorized_keys
fi

mkdir -p /etc/jenkins_jobs
cd /opt/stack/config-tools/

